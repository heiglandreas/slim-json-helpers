{
  "name" : "org_heigl/slim-json-helpers",
  "description" : "Clone of aporat/slim-json-helpers | Slim3 JSON Request & Response Helpers",
  "type" : "library",
  "authors" : [{
    "name" : "Adar Porat",
    "email" : "adar.porat@gmail.com",
    "homepage" : "http://github.com/aporat"
  }
  ],
  "keywords" : [
    "slim",
    "json"
  ],
  "homepage" : "https://github.com/heiglandreas/slim-json-helpers",
  "license" : [
    "Apache-2.0"
  ],
  "replace" : {
    "aporat/slim-json-helpers": "*"
  },
  "conflict" : {
    "aporat/slim-json-helpers": "*"
  },
  "require" : {
    "php" : "^7.2 | ^8.0",
    "slim/slim": "~3",
    "psr/container": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 | ^9.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "JsonHelpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonHelpersTest\\": "tests/"
    }
  },
  "support" : {
    "email" : "andreas@heigl.org",
    "issues" : "https://github.com/heiglandreas/slim-json-helpers/issues"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
